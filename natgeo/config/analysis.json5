{
  analysis: {
    content: { selector: [
      '.site-content, .ng-container, .ng-container-large',
      '.archive-posts, main article.ng-width-large-2-3',
      '.ng-edu-homepage-wrapper > .d-flex.container, main.container > article, main:not([class]) .col-lg-8',
      '.post-template-default .entry-content, body:not(.post-template-default) .site-main',
      'article.flex-column, [data-test-id="row"]',
    ]},
    properties: {
      'content.cms': [
        'tech.CMS.[0]',
        'tech.Blogs.[0]',
      ],
      'content.type': [
        {
          source: 'parsed.path.[2]',
          in: ['explorer-magazine', 'event'],
        },
        {
          source: 'parsed.path.[0]',
          in: ['glossary', 'resource', 'search', 'activity', 'tag', 'category', 'projects', 'quiz', 'unit', 'lesson' ],
        },
        'data.meta.og.type',
      ],
      'paged': [
        { source: 'parsed.searchParams.page', gt: 0, value: true },
        { source: 'parsed.pathname', matching: '/**/page/[[:digit:]]', value: true }
      ],
      'content.title': [
        'data.meta.og.title', // OpenGraph
        'data.title',         // HEAD description tag
        'data.info.Title',    // PDF metadata
      ],
      'content.description': [
        'data.meta.og.description', // OpenGraph
        'data.meta.description',    // HEAD description tag
        'data.schemaOrg.WebPage.description',    // SchemaOrg
        'data.dc.description',       // PDF metadata
      ],
      'content.author': [
        'data.meta.author',
      ], 
      'content.date': [
        'data.meta.article.published_time',     // Many pages put the info here
        'data.schemaOrg.WebPage.datePublished', // The jobs site exposes Schema.org data!
        'data.info.CreationDate',               // PDF files
        'data.format.creationTime',             // audio
        'data.ModifyDate'                       // many photos
      ],
    }
  }
}