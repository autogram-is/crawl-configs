{
  analysis: {
    content: { selector: "div#main, main#main-content, main#page-content" },
    properties: {
      'content.cms': [
        'tech.CMS.[0]',
        'tech.Blogs.[0]',
      ],
      
      'content.videos': { source: 'body', selector: 'div.inline-video-player__canvas[id], section.c19-video-hero__wrapper', count: true, fallback: 0 },
      'content.marketoForms': { source: 'body', selector: 'form[data-marketo-form-id]+*', count: true, fallback: 0 },
      'content.otherForms': { source: 'body', selector: 'form:not([data-marketo-form-id])+*', count: true, fallback: 0 },
      'content.iframes': { source: 'body', selector: 'iframe', attribute: 'src' },
      'content.language': [
        { 'source': 'body', 'selector': 'html', 'attribute': 'data-cms-lang' },
        { 'source': 'body', 'selector': 'html', 'attribute': 'lang' },
      ],
      'content.uuid': ['data.attributes.data.article-uuid'],
      'content.pageId': ['data.meta.page-id'],
      'content.tags': ['data.meta.data-tags'],
      'content.taxonomyTags': ['data.meta.data-tagTaxonomy'],
      'content.type': ['data.meta.resource-type-tag'],
      'content.title': [
        'data.meta.og.title', // OpenGraph
        'data.info.Title',    // PDF metadata
        'data.title',         // HEAD description tag
      ],
      'content.description': [
        'data.meta.og.description', // OpenGraph
        'data.meta.description',    // HEAD description tag
        'data.schemaOrg.WebPage.description',    // SchemaOrg
        'data.dc.description',       // PDF metadata
      ],
      'content.date': [
        'data.meta.datePublished',
        'data.meta.article.published_time',
        'data.schemaOrg.Article.datePublished',
        'data.schemaOrg.WebPage.datePublished',
        'data.info.CreationDate',               // PDF files
        'data.format.creationTime',             // audio
        'data.ModifyDate'                       // many photos
      ],
    }
  }
}