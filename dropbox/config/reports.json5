{
  reports: {
    default: {
      name: 'Dropbox English Pages',
      group: 'dropbox',
      description: 'Page, media, and error summaries',
      queries: {
        'Overview': {
          parent: 'dropbox_summary',
          aggregates: [
            { path: 'mime', function: 'collect' },
          ],
          filters: [
            { path: 'mime', eq: 'text/html' },
          ],
        },
        'Other Pages': {
          parent: 'dropbox_pages',
          filters: [
            { path: 'content.language', in: [null, 'en', 'en-us'] },
            { path: 'parsed.path[0]', contains: '__-__', type: 'string', negate: true },
          ],
        },
      },
      alterData: {
        'Other Pages': {
          action: 'split',
          property: 'Site',
          mustMatch: [
            'www.dropbox.com',
            'experience.dropbox.com',
            'blog.dropbox.com',
          ]
        }
      },
    },
    localized: {
      name: 'Dropbox International Pages',
      group: 'dropbox',
      description: 'Page, media, and error summaries',
      queries: {
        'Overview': {
          parent: 'dropbox_summary',
          aggregates: [
            { path: 'mime', function: 'collect' },
          ],
          filters: [
            { path: 'mime', eq: 'text/html' },
          ],
        },
        'All Pages': {
          parent: 'dropbox_pages',
          filters: [
            { path: 'content.language', in: [null, 'en', 'en-us'], negate: true },
          ],
        },
      },
      alterData: {
        'All Pages': {
          action: 'split',
          property: 'Language',
        }
      },
    },
    media: {
      name: 'dropbox Downloadable Files',
      group: 'dropbox',
      description: 'Non-HMTL files linked by Dropbox sites',
      queries: {
        'Overview': {
          parent: 'dropbox_summary',
          aggregates: [
            { path: 'mime', function: 'collect' },
          ],
          filters: [
            { path: 'mime', eq: 'text/html', negate: true },
          ],
        },
        'PDFs': {
          parent: 'dropbox_media',
          filters: [{ path: 'mime', eq: 'application/pdf' }],
          return: [
            { name: 'Title', path: 'content.title' },
            { name: 'Date', path: 'content.date' },
            { name: 'Pages', path: 'data.pages' },
            { name: 'Words', path: 'content.readability.words' },
            { name: 'Language', path: 'data.info.Language' },
            { name: 'Links', path: 'content.urls', function: 'LENGTH' },
          ]
        },
        'Photos': {
          parent: 'dropbox_media',
          filters: [{ path: 'mime', in: ['image/png', 'image/jpeg'] }],
          return: [
            { name: 'Date', path: 'data.ModifyDate' },
            { name: 'Height', path: 'data.height' },
            { name: 'Width', path: 'data.width' },
            { name: 'Copyright', path: 'data.Copyright' },
          ]
        },
        'Audio': {
          parent: 'dropbox_media',
          filters: [{ path: 'mime', eq: 'audio/mpeg' }],
          return: [
            { name: 'Duration', path: 'data.format.duration' },
          ]
        },
        'Videos': {
          parent: 'dropbox_media',
          filters: [{ path: 'mime', eq: 'video/mp4' }],
          return: [
            { name: 'Date', path: 'data.format.creationTime' },
            { name: 'Duration', path: 'data.format.duration' },
          ]
        },
        'Other Files': {
          parent: 'dropbox_media',
          filters: [{ path: 'mime', in: ['application/pdf', 'image/png', 'image/jpeg', 'audio/mpeg', 'video/mp4'], negate: true }]
        },
      }
    }
  }
}