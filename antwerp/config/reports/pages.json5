{
  reports: {
    default: {
      name: 'Pages',
      group: 'project',
      description: 'Page, media, and error summaries',
      queries: {
        'Summary': {
          parent: 'project_summary',
          aggregates: [
            { path: 'mime', function: 'collect' },
          ],
          filters: [
            { path: 'mime', eq: 'text/html' },
          ],
        },
        'All Pages': {
          parent: 'project_pages',
          filters: [
            { path: 'mime', eq: 'text/html' },
            { path: 'parsed.domain', eq: 'antwerpen.be' },
          ],
        },
      },
      alterData: {
        'All Pages': {
          action: 'split',
          property: 'Site',
          mustMatch: [
            'www',
            'assets',
            'www.zorgbedrijf',
            'visit',
            'ebesluit',
          ]
        },
      },
    }
  }
}
